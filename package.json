{
    "name": "@proddi/map-components",
    "version": "0.0.1",
    "license": "MIT",
    "module": "src/index.js",
    "main": "src/index.js",
    "repository": {
        "type": "git",
        "url": "https://github.com/proddi/map-components.git"
    },
    "scripts": {
        "build": "./node_modules/.bin/webpack --config webpack.config.js",
        "build:prod": "NODE_ENV=production npm run build",
        "build:docs": "rm -rf docs; ./node_modules/.bin/esdoc -c esdoc.json",
        "build:docs-templates": "rm -rf docs; ./node_modules/.bin/esdoc -c esdoc-templates.json",
        "push:docs": "./node_modules/.bin/gh-pages -d build/docs/ -m \"Auto-generated commit for documentation\"",
        "server": "./node_modules/.bin/polymer serve"
    },
    "dependencies": {
        "@polymer/iron-icons": "^3.0.1",
        "@polymer/paper-icon-button": "^3.0.2",
        "@polymer/paper-item": "^3.0.1",
        "@polymer/paper-spinner": "^3.0.1",
        "lit-html": "^1.0.0-rc.2"
    },
    "devDependencies": {
        "esdoc": "^1.1.0",
        "esdoc-standard-plugin": "^1.0.0",
        "esdoc-publish-html-templates-plugin": "file:esdoc-publish-html-templates-plugin",
        "@google-web-components/google-youtube": "^3.0.1",
        "webpack": "^4.28.3",
        "webpack-cli": "^3.2.0",
        "polymer-cli": "^1.9.5"
    }
}
